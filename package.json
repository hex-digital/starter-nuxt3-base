{
  "packageManager": "yarn@1.22.9",
  "browserslist": [
    "> 1%",
    "last 4 versions",
    "Firefox ESR",
    "not edge <= 18",
    "not ie 11",
    "not op_mini all",
    "not dead"
  ],
  "scripts": {
    "install:all": "for D in */; do yarn --cwd \"${D}\"; done",
    "lint": "eslint --cache src",
    "lint:fix": "eslint --cache --fix src",
    "typecheck": "yarn --cwd src nuxi typecheck --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@hexdigital/config-commitlint": "^1.0.1",
    "@hexdigital/eslint-config": "^1.0.6",
    "eslint": "^8.23.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3"
  }
}
